{
  "tasks": {
    "dev": "deno run --allow-env --allow-read --allow-write --allow-ffi --allow-run --allow-sys --allow-net --node-modules-dir npm:vite"
  },
  "imports": {
    "@discord/embedded-app-sdk": "npm:@discord/embedded-app-sdk@^1.8.1",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.2",
    "@types/react": "npm:@types/react@^19.0.2",
    "@types/react-dom": "npm:@types/react-dom@^19.0.2",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.4",
    "phaser": "npm:phaser@^3.87.0",
    "react": "npm:react@^19.0.0",
    "react-dom": "npm:react-dom@^19.0.0",
    "vite": "npm:vite@^6.0.5"
  },
  "compilerOptions": {
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  }
}